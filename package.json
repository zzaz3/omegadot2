  },
  "dependencies": {
    "@okta/jwt-verifier": "0.0.11",
    "@okta/okta-auth-js": "^1.13.0",
    "@okta/okta-react": "0.0.12",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "dataurl-to-blob": "0.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
